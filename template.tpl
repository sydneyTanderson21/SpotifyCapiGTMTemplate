___SANDBOXED_JS_FOR_SERVER___

// Sandbox Javascript API imports
const sendHttpRequest = require('sendHttpRequest');
const logToConsole = require('logToConsole');
const makeString = require('makeString');
const JSON = require('JSON');
const getTimestampMillis = require('getTimestampMillis');
const getAllEventData = require('getAllEventData');
const getRequestHeader = require('getRequestHeader');
const Math = require('Math');
const makeNumber = require('makeNumber');
const getType = require('getType');


// Dynamic variables
const eventData = getAllEventData();
const conversion_events_builder = {};
const conversion_events_3P = {};

if(!data.connectionId || !data.authToken) {
  return data.gtmOnFailure();
}

// logic for building Conversion 3P event object and validating user data
const conversion_event = mapEvent(eventData);
if (!validUserData(conversion_event)){
  logToConsole('No conversion event was sent to CAPI. Please verify that user fields are provided');
  return data.gtmOnFailure();
}

conversion_events_builder.capi_connection_id = data.connectionId;
conversion_events_builder.events = [conversion_event];

conversion_events_3P.third_party_source = "google_tag_manager";
conversion_events_3P.conversion_events = conversion_events_builder;

// Preparing the HTTP POST call to CAPI endpoint
const endpointUrl = "https://capi.spotify.com/capi-3p/events/";
const postBody = JSON.stringify(conversion_events_3P);
//logToConsole('POST BODY:', postBody);
const requestHeaders = {
  'Authorization': 'Bearer ' + data.authToken,
  'Content-Type': 'application/json'
  };

sendConversionData(endpointUrl, requestHeaders, postBody);


function sendConversionData(endpointUrl, requestHeaders, postBody) {
  sendHttpRequest(endpointUrl, {
  headers: requestHeaders,
  method: 'POST',
  }, postBody)
    .then(response => {
    if (response.statusCode >= 200 && response.statusCode < 300) {
      logToConsole('Event sent successfully.');
      data.gtmOnSuccess();
    } else{
      logToConsole('Failed to send event: ' + response.body);
      data.gtmOnFailure();
    } 
  }).catch(error => {
    logToConsole('Error sending event, caused by: ' + error.reason);
    data.gtmOnFailure();

  });
}

function validUserData(event) {
  var data_is_valid_flag = true; 
  if (!event || !event.user_data) {
    logToConsole('INVALID USER DATA: No user data object found in event');
    return false;
  }
  
  const userData = event.user_data;
  let hasValidIdentifier = false;
  
  if (userData.ip_address) {
    hasValidIdentifier = true;
  }
  
  if (userData.device_id) {
    hasValidIdentifier = true;
  }
  
  if (userData.hashed_emails && userData.hashed_emails.length > 0) {
    hasValidIdentifier = true;
  }
  
  if (userData.hashed_phone_number) {
    hasValidIdentifier = true;
  }
  
 if (hasValidIdentifier) {
    return true;
  } else {
    logToConsole('INVALID USER DATA: At least one identifier type (IP address, device ID, hashed email, or hashed phone number) is required');
    return false;
  }
}

function mapEvent(eventData) {
  let mappedData = {};
  mappedData.event_name = getEventName(eventData);
  
  let timestamp = eventData.event_time != null ? eventData.event_time : getTimestampMillis();
  mappedData.event_time = {
    seconds: formatSeconds(timestamp),
    nanos: 0
  };

  mappedData.action_source = data.actionSource || 'WEB';
  
  mappedData.opt_out_targeting = data.opt_out_targeting || false;
  
  mappedData.event_source_url = eventData.page_location || eventData.page_referrer || getRequestHeader('referer');
  
  mappedData.event_id = eventData.event_id; 

  mappedData = addUserData(eventData, mappedData);
  mappedData = addEventDetails(eventData, mappedData);
  
   if (data.serverParameters) {
    data.serverParameters.forEach(d => {
      if (d.value) {
        if(d.name === 'event_time'){
          const formattedTime = formatTime(d.value, mappedData.event_time);
          mappedData[d.name] = formattedTime;
        }else{
          mappedData[d.name] = d.value;
        }
      }
    });
  }
  
  return mappedData;
}


//to do: add fields for first and last name
function addUserData(eventData, mappedData) {
  mappedData.user_data = {};
  
  mappedData.user_data.ip_address = eventData.ip_override || "";
  mappedData.user_data.user_agent = eventData.user_agent || "";

  // Pull user data if available 
  if (eventData.user_data != null) {
    let userData = eventData.user_data;
    
    mappedData.user_data.hashed_emails = userData.sha256_email_address ? [userData.sha256_email_address] : [];
    mappedData.user_data.hashed_phone_number = userData.sha256_phone_number || "" ;
    mappedData.user_data.device_id = userData.client_id ||  "";
  }
  
   if (data.userDataParameters) {
    data.userDataParameters.forEach(d => {
      if (d.value) {
        if(d.name === 'hashed_emails'){
          mappedData.user_data[d.name] = [d.value];
        }else{
        mappedData.user_data[d.name] = d.value;
        }
      }
    });
  }
  
  return mappedData;
}

function addEventDetails(eventData, mappedData) {
  mappedData.event_details = {};

  //assuming we have currency and value being sent, but this isnt guaranteed 
  mappedData.event_details.amount = eventData.value || eventData.price;
  
  mappedData.event_details.currency = (eventData.currency != null) ? eventData.currency : "";
  
  mappedData.content_name = (data.contentCategory != null) ? data.contentCategory : "";
  
  mappedData.content_category = (data.contentName != null) ? data.contentName : ""; 
  
  return mappedData;

}

function getEventName(eventData) {
  if (data.inheritEventName === 'inherit') {
    let eventName = eventData.event_name;
    let gaToSnapEventName = {
      'page_view': 'Page_View',
      'gtm.dom': 'Page_View',
      'add_to_cart': 'Add_Cart',
      'sign_up': 'Sign_Up',
      'purchase': 'Purchase',
      'view_item': 'View_Product',
      'begin_checkout': 'Start_Checkout',
      'generate_lead': 'Sign_Up',
      'gtm4wp.addProductToCartEEC': 'Add_Cart',
      'gtm4wp.productClickEEC': 'View_Product',
      'gtm4wp.orderCompletedEEC': 'Purchase'
    }; 
     
    if(gaToSnapEventName[eventName]) {
      return gaToSnapEventName[eventName];
    } else {
      logToConsole('Event name "'+ eventName + '" not found in our predefined mapping. Please either create a custom event or select one of the available standard options');
      return data.gtmOnFailure();
    }
  }
  //logToConsole('Custom Event Name Value:', data.eventNameCustomValue);
  return data.inheritEventName === 'custom' ? data.eventNameCustom : data.eventNameStandard;
}

function formatSeconds(timestamp) {
  // Convert to seconds if in milliseconds
  const timestampSec = timestamp > 9999999999 ? Math.floor(timestamp / 1000) : timestamp;
  return timestampSec;
}

function formatTime(time, originalEventTime) {
   // check if string looks like RFC 3339 format
  if (getType(time) === 'string' && time.indexOf('T') > 0 && time.indexOf('Z') > 0) {
    logToConsole('Using provided RFC 3339 timestamp: ' + time);
    return time;
  }
 
  if (getType(time) === 'string') {
    const numericTimestamp = makeNumber(time);
    if (numericTimestamp === numericTimestamp) {
      return {
        seconds: formatSeconds(numericTimestamp),
        nanos: 0
      };
    } 
  }
  
  if (getType(time) === 'number') {
    return {
      seconds: formatSeconds(time),
      nanos: 0
    };
  }
  
    logToConsole('Warning: The provided event_time value "' + time + '" is invalid. ' +
               'Please provide either RFC 3339 format (YYYY-MM-DDTHH:MM:SSZ) or a numeric timestamp. ' +
               'Using current time instead.');
  
  return originalEventTime;
}


